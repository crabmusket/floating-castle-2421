$('##{rawJS formId}').submit(function() {
	var values = {};
	$(this).find(':input').each(function() {
		if(this.name) {
			values[this.name] = $(this).val();
		}
	});

	$.ajax({
		type: 'POST',
		url: '@{MessagesR}',
		data: values,
		dataType: 'json',
		statusCode: {
			200: function(response) {
				if(response.error) {
					alert(response.error);
				}
			},
		},
	});
	return false;
});
